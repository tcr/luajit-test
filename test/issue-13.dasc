#include <stdint.h>

// DynASM directives.
|.arch thumb
|.actionlist actions

#include "test.h"







typedef struct {
	uint32_t key;
	uint32_t val;
} lua_State;


COMPARE_JIT(immtest,
__asm__ (
   " ldrd r0, r1, [r11, #0] \n\t"
   " ldrd r2, r3, [r11, #4] \n\t"
), {
  |.type NODE, lua_State
  |.define CARG1, r0
  |.define CARG2, r1
  |.define CARG3, r2
  |.define CARG4, r3
  |.define INS, r11
	|  ldrd CARG1, CARG2, NODE:INS->key
	|   ldrd CARG3, CARG4, NODE:INS->val
})






int main ()
{
	printf("1..1\n");
	immtest_jit();
	printf("# done\n");
	return 0;
}
